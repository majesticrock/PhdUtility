@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Dependencies required by SymbolicOperators
find_dependency(ZLIB REQUIRED)
find_dependency(Boost REQUIRED COMPONENTS serialization iostreams)

if (NOT TARGET Utility)
    if (EXISTS "${CMAKE_CURRENT_LIST_DIR}/../Utility/UtilityConfig.cmake")
        include("${CMAKE_CURRENT_LIST_DIR}/../Utility/UtilityConfig.cmake")
    else()
        find_dependency(Utility REQUIRED)
    endif()
endif()

# Include targets
include("${CMAKE_CURRENT_LIST_DIR}/SymbolicOperatorsTargets.cmake")

# Ensure that the target is available
check_required_components(SymbolicOperators)
